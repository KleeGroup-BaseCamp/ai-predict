package io.vertigo.ai.example.iris

create Task TkGetIris {  
    className : "io.vertigo.basics.task.TaskEngineSelect"
    request : "
            select * from IRIS
             "
    out dtcIris    {domain : DoDtIris         cardinality: "*" }
}

create Task TkLoadIris {  
    className : "io.vertigo.basics.task.TaskEngineSelect"
    request : "
            select * from IRIS where ID in (#irisIds.rownum#);
             "
    in irisIds     {domain : DoId         cardinality: "*" }
    out dtcIris    {domain : DoDtIris         cardinality: "*" }
}

create Task TkBulkCreateIris {  
    className : "io.vertigo.basics.task.TaskEngineProcBatch"
    request : "
            INSERT INTO IRIS (ID, SEPALLENGHT, SEPALWIDTH, PETALLENGTH, PETALWIDTH, VARIETY) values (nextval('SEQ_EQUIPEMENT'), #irisList.sepalLength#, #irisList.sepalWidth#, #irisList.petalLength#, #irisList.petalWidth#, #irisList.variety#)   
             "
    in irisList    {domain : DoDtIris         cardinality: "*" }
}
